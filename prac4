#include <stdio.h>

int countEqualDivisors(int n) {
    int count = 0;
    
    
    for (int m = 1; m < n; m++) {
        if ((n / m) == (n % m)) {
            count++;
        }
    }
    
    return count;
}

int main() {
    int n;
    
    printf("Введіть натуральне число n (1 < n < 150): ");
    scanf("%d", &n);
    
    if (n <= 1 || n >= 150) {
        printf("Число n повинно бути в діапазоні 1 < n < 150.\n");
        return 1;
    }
    
    int result = countEqualDivisors(n);
    printf("Кількість рівних дільників числа %d: %d\n", n, result);
    
    return 0;
}
